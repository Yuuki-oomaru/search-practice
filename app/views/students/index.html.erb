<h1>学生検索</h1>
<%= search_form_for(@q, url:search_path) do |f| %>
<%= f.label :name_cont, '名前' %>
<%= f.search_field :name_cont %>
<%= f.submit %>
<% end %>
<table>
<% @students.each do |student| %>
  <tr>
    <td><%= student.name %></td>
    <td><%= student.sex %></td>
    <td><%= student.age %></td>
    <td><%= student.department.name %></td>
  </tr>
<% end %>
</table>
